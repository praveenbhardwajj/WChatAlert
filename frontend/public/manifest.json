{
  "manifest_version": 3,
  "name": "WChatAlert",
  "version": "1.0",
  "description": "WhatsApp Sender Alert",

 "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    }
  },

  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["http://localhost:8080/*"]
}